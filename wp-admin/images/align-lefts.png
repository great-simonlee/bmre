			$precat = $tp_pname_arr[0];
		}
		
		if(function_exists("preg_split")){
			$tpnameArr = preg_split('/\s+/us', $pname);		
		}else{
			$tpnameArr = explode(' ', $pname);
		}
		
		$pnameArr = array();
		foreach($tpnameArr as $tpvs){
			$tpvs = trim($tpvs);
